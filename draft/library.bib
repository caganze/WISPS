@article{Hayden2015,
abstract = {Using a sample of 69,919 red giants from the SDSS-III/APOGEE Data Release 12, we measure the distribution of stars in the [{\$}\backslashalpha{\$}/Fe] vs. [Fe/H] plane and the metallicity distribution functions (MDF) across an unprecedented volume of the Milky Way disk, with radius {\$}3{\textless}R{\textless}15{\$} kpc and height {\$}|z|{\textless}2{\$} kpc. Stars in the inner disk ({\$}R{\textless}5{\$} kpc) lie along a single track in [{\$}\backslashalpha{\$}/Fe] vs. [Fe/H], starting with {\$}\backslashalpha{\$}-enhanced, metal-poor stars and ending at [{\$}\backslashalpha{\$}/Fe]{\$}\backslashsim0{\$} and [Fe/H]{\$}\backslashsim+0.4{\$}. At larger radii we find two distinct sequences in [{\$}\backslashalpha{\$}/Fe] vs. [Fe/H] space, with a roughly solar-{\$}\backslashalpha{\$} sequence that spans a decade in metallicity and a high-{\$}\backslashalpha{\$} sequence that merges with the low-{\$}\backslashalpha{\$} sequence at super-solar [Fe/H]. The location of the high-{\$}\backslashalpha{\$} sequence is nearly constant across the disk, however there are very few high-{\$}\backslashalpha{\$} stars at {\$}R{\textgreater}11{\$} kpc. The peak of the midplane MDF shifts to lower metallicity at larger {\$}R{\$}, reflecting the Galactic metallicity gradient. Most strikingly, the shape of the midplane MDF changes systematically with radius, with a negatively skewed distribution at {\$}3{\textless}R{\textless}7{\$} kpc, to a roughly Gaussian distribution at the solar annulus, to a positively skewed shape in the outer Galaxy. For stars with {\$}|z|{\textgreater}1{\$} kpc or [{\$}\backslashalpha{\$}/Fe]{\$}{\textgreater}0.18{\$}, the MDF shows little dependence on {\$}R{\$}. The positive skewness of the outer disk MDF may be a signature of radial migration; we show that blurring of stellar populations by orbital eccentricities is not enough to explain the reversal of MDF shape but a simple model of radial migration can do so.},
archivePrefix = {arXiv},
arxivId = {1503.02110},
author = {Hayden, Michael R. and Bovy, Jo and Holtzman, Jon A. and Nidever, David L. and Bird, Jonathan C. and Weinberg, David H. and Andrews, Brett H. and Majewski, Steven R. and Prieto, Carlos Allende and Anders, Friedrich and Beers, Timothy C. and Bizyaev, Dmitry and Chiappini, Cristina and Cunha, Katia and Frinchaboy, Peter and Garc{\'{i}}a-Her{\'{n}}andez, D. A. and {Garc{\'{i}}a P{\'{e}} Rez}, Ana E. and Girardi, L{\'{e}}o and Harding, Paul and Hearty, Fred R. and Johnson, Jennifer A. and M{\'{e}}sz{\'{a}}ros, Szabolcs and Minchev, Ivan and O'Connell, Robert and Pan, Kaike and Robin, Annie C. and Schiavon, Ricardo P. and Schneider, Donald P. and Schultheis, Mathias and Shetrone, Matthew and Skrutskie, Michael and Steinmetz, Matthias and Smith, Verne and Wilson, John C. and Zamora, Olga and Zasowski, Gail},
doi = {10.1088/0004-637X/808/2/132},
