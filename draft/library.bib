@article{Testi2016,
abstract = {The study of the properties of disks around young brown dwarfs can provide important clues on the formation of these very low mass objects and on the possibility of forming planetary systems around them. The presence of warm dusty disks around brown dwarfs is well known, based on near- and mid-infrared studies. High angular resolution observations of the cold outer disk are limited, we used ALMA to attempt a first survey of young brown dwarfs in the rho-Ophiuchi star forming region with ALMA. All 17 young brown dwarfs in our sample were observed at 890 um in the continuum at {\~{}}0.5" angular resolution. The sensitivity of our observations was chosen to detect {\~{}}0.5 MEarth of dust. We detect continuum emission in 11 disks (65{\%} of the total), the estimated mass of dust in the detected disks ranges from {\~{}}0.5 to {\~{}}6 MEarth. These disk masses imply that planet formation around brown dwarfs may be relatively rare and that the supra-Jupiter mass companions found around some brown dwarfs are probably the result of a binary system formation. We find evidence that the two brightest disks in rho-Oph have sharp outer edges at R{\~{}}25 AU, as opposite to disks around Taurus brown dwarfs. This difference may suggest that the different environment in rho-Oph may lead to significant differences in disk properties. A comparison of the Mdisk/Mstar ratio for brown dwarf and solar-mass systems also shows a possible deficit of mass in brown dwarfs, which could support the evidence for dynamical truncation of disks in the substellar regime. These findings are still tentative and need to be put on firmer grounds by studying the gaseous disks around brown dwarfs and by performing a more systematic and unbiased survey of the disk population around the more massive stars.},
archivePrefix = {arXiv},
arxivId = {1606.06448},
author = {Testi, L. and Natta, A. and Scholz, A. and Tazzari, M. and Ricci, L. and Monsalvo, I. de Gregorio},
eprint = {1606.06448},
file = {:Users/caganze/Library/Application Support/Mendeley Desktop/Downloaded/Testi et al. - 2016 - Brown dwarf disks with ALMA evidence for truncated dust disks in Ophiuchus.pdf:pdf},
month = {jun},
title = {{Brown dwarf disks with ALMA: evidence for truncated dust disks in Ophiuchus}},
url = {http://arxiv.org/abs/1606.06448},
year = {2016}
}
