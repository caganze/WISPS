keywords = {binaries: general,brown dwarfs,stars: fundamental parameters,stars: individual: 2MASS J03202839--0446358,stars: low mass},
month = {mar},
pages = {4621--4626},
title = {{An Age Constraint for the Very Low-Mass Stellar/Brown Dwarf Binary 2MASS J03202839-0446358AB}},
url = {http://arxiv.org/abs/0903.3440 http://dx.doi.org/10.1088/0004-6256/137/6/4621},
