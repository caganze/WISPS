@article{Steidel1996,
abstract = {We report the discovery of a substantial population of star--forming galaxies at {\$}3.0 \backslashsimlt z \backslashsimlt 3.5{\$}. These galaxies have been selected using color criteria sensitive to the presence of a Lyman continuum break superposed on an otherwise very blue far-UV continuum, and then confirmed with deep spectroscopy on the W. M. Keck telescope. The surface density of galaxies brighter than R=25 with 3 {\textless} z {\textless} 3.5 is {\$}0.4\backslashpm0.07{\$} galaxies arcmin{\$}{\^{}}{\{}-2{\}}{\$}, approximately 1.3$\backslash${\%} of the deep counts at these magnitudes; this value applies both to ``random'' fields and to fields centered on known QSOs. The corresponding co-moving space density is approximately half that of luminous ({\$}L \backslashsimgt L{\^{}}{\{}\backslashast{\}}{\$}) present--day galaxies. Our sample of {\$}z {\textgreater} 3{\$} galaxies is large enough that we can begin to detail the spectroscopic characteristics of the population as a whole. The spectra of the {\$}z{\textgreater}3{\$} galaxies are remarkably similar to those of nearby star-forming galaxies, the dominant features being strong low--ionization interstellar absorption lines and high--ionization stellar lines, often with P-Cygni profiles characteristic of Wolf-Rayet and O--star winds. Lyman {\$}\backslashalpha{\$} emission is generally weak ({\$}{\textless} 20{\$} $\backslash$AA$\backslash$ rest equivalent width) and is absent for {\textgreater}50{\%} of the galaxies. The star formation rates, measured directly from the far-UV continua, lie in the range 4-25 {\$}h{\_}{\{}50{\}}{\^{}}{\{}-2{\}}{\$} M{\$}\backslashsun{\$} yr{\$}{\^{}}{\{}-1{\}}{\$} for {\$}q{\_}0=0.5{\$}. Together with the morphological properties of the {\$}z{\textgreater}3{\$} galaxy population, which we discuss in a companion paper (Giavalisco $\backslash$et 1996), all of these findings strongly suggest that we have identified the high-redshift counterparts of the spheroid component of present--day luminous galaxies. In any case, it is clear that massive galaxy formation was already well underway by {\$}z \backslashsim 3.5{\$}. (shortened abstract). arch-ive/yymmnnn},
archivePrefix = {arXiv},
arxivId = {astro-ph/9602024},
author = {Steidel, C. and Giavalisco, M. and Pettini, M. and Dickinson, M. and Adelberger, K.},
doi = {10.1086/310029},
eprint = {9602024},
issn = {0004-637X},
journal = {Astrophysical Journal Letters v.462, p.L17},
keywords = {COSMOLOGY: OBSERVATIONS,GALAXIES: DISTANCES AND REDSHIFTS,GALAXIES: EVOLUTION,GALAXIES: FORMATION},
month = {feb},
pages = {L17},
